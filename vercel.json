{
    "framework": "django",
    "build": {
        "src":"quizz_django/wsgi.py",
        "use":"@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime":"python3.10.12" }
    },
    "routes": [
        {
            "src":"/(.*)",
            "dest":"quizz_django/wsgi.py"
        }
    ]
}